/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.top{
  width: 30px;
  height: 30px;
  background-color: #aaa;
  border-radius: 50%;
  overflow: hidden;
  zoom:1;
}
.swiper image{
  width: 100%;
  height: 100%;
}
.jianxi{
  width: 100%;
  height: 15px;
  background-color: #f7f7f7;
  margin-top: 5px;
}
.biaoti{
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #e6e6e6;
}
.biaoti image{
  float: left;
  margin-right: 10px;
}
.biaoti .word{
  float: left;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333;
}
.biaoti .strong{
  font-weight: 800
}
.box{
  overflow: hidden;
  zoom:1;
  width: 95%;
  margin: 0 auto;
 background-image: url("http://localhost:8080/waimai/upload/image/img/bg.jpg")
}
.box .li{
  overflow: hidden;
  zoom:1;
  padding: 10px 0;
  border-bottom: 1px solid #e6e6e6;
}
.box .li image{
  float: left;
  width: 30%;
  height: 90px;
  border-radius: 20px;
  
}
.box .li .you{
  float: right;
  width: auto;
  overflow: hidden;
  zoom:1;
}
.box .li .you .tp{
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-size: 16px;
  color: #333;
  overflow: hidden;
  zoom:1;
  display: block;
}
.box .li .you .bt{
  height: 50px;
  line-height: 25px;
  overflow: hidden;
  zoom:1;
  font-size: 14px;
  color: #666;
  display: block;
  margin-right:80px;
}

.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}

.tit{
  overflow: hidden;
  zoom:1;
  padding-left: 10px;
  padding-top: 10px;
  padding-bottom: 10px;

}
.tit image{
  display: block;
  width: 60%;
}
.hui{
  height: 10px;
  background-color: #f7f7f7;
  margin-bottom: 10px;
}
form{
  display: block;
  width: 96%;
  height: auto;
  position: relative;
  margin: 0 auto 40px;
  padding: 30px 0;
  border-radius: 5px;
  overflow: hidden;zoom:1;
}
form .bg{
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -99;
}

form .add{
  display: block;
  width: 40px;
  height:40px;
 
  z-index: -99;
}

form .tp{
  overflow: hidden;
  zoom:1;
  width: 60%;
  margin: 0 auto 20px;
}
form .tp image{
  float: left;
  width: 50px;
}
form .tp image.girl{
  float: right;
  width: 60px;
}
form .radio_k{
  width:47%;
  margin: 0 auto -10px 0px;
  overflow: hidden;
  zoom:1;
}
form .radio_k label{
  float: left;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
}
form .radio_k label.la2{
  float: right;
}
form .hang{
  overflow: hidden;
  zoom:1;
  width:90%;
  margin: 0 auto 10px;
}
form .hang .z{
  float: left;
  width: 80px;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333;
}
form .hang input{
  float: left;
  width: 50%;
  height: 38px;
  line-height: 38px;
  padding: 0 5px;
  border: 1px solid #13e6e6;
  border-radius: 5px;
  background-color: #fff;
  font-size: 14px;
  color: #333;
}
form .hang .z text{
  opacity: 0;
}
form .hang .y{
  float: right;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333;
}
form .hang .x{
  float: left;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333;
  margin-left:8px;
 
}
form .hang .xx{
  float: left;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333;
   margin-left:8px;
}

form .but{
  display: block;
  width: 80%;
  height: 45px;
  line-height: 45px;
  margin: 0 auto;
  position: relative;
  border-radius: 25px;
  overflow: hidden;
  zoom:1;
  margin-top: 30px;
}
form .but text{
  position: relative;
  z-index: 100;
}
form .but image{
  width: 100%;
  height: 45px;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 99;
}
form .but button{
  display: block;
  width: 100%;
  height: 45px;
  line-height: 45px;
  background: none;
  text-align: center;
  font-size: 16px;
 
  color: #fff;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 999;
  border: none;

}
